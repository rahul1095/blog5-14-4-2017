<h3>Edit Comment</h3>


<%#= form_for([@article, @comment]) do |f| %>
<%= form_for(:comment, url: "/blogs/#{@blog.id}/comments") do |f| %>

  <p>
    <%= f.label :commenter %><br>
    <%= f.text_field :commenter %>
  </p>
  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>